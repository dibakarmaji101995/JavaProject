<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="Ticket" type="TicketType"/>
	<xs:complexType name="TicketType">
		<xs:sequence>
			<xs:element name="TicketNumber" type="xs:int"/>
			<xs:element name="PNRNo" type="xs:int"/>
			<xs:element name="TrainName" type="xs:string"/>
			<xs:element name="TrainNumber" type="xs:int"/>
			<xs:element name="From" type="xs:string"/>
			<xs:element name="To" type="xs:string"/>
			<xs:element name="JourneyDate" type="xs:date"/>
			<xs:element name="PassengerName" type="xs:string"/>
			<xs:element name="Email" type="xs:string"/>
			<xs:element name="PhoneNumber" type="xs:long"/>
			<xs:element name="status" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
